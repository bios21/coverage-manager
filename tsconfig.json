{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015", "es2016", "es2017"],
    "sourceMap": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
